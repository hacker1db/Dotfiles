#!/bin/bash
#Generated by JetBrains Toolbox 2.1.0.18144 at 2023-11-09T07:22:39.798643

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/David.Walters@AlaskaAir.com/Applications/Writerside.app/Contents/MacOS/writerside" $wait --args "${intellij_args[@]}"