#Generated by JetBrains Toolbox 1.24.11947 at 2022-05-12T18:22:21.589244

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/hacker1db/Library/Application Support/JetBrains/Toolbox/apps/Rider/ch-0/221.5591.20/Rider.app/Contents/MacOS/rider" $wait --args "${ideargs[@]}"