#Generated by JetBrains Toolbox 1.21.9712 at 2021-11-01T20:51:51.302149

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/hacker1db/Library/Application Support/JetBrains/Toolbox/apps/Rider/ch-0/213.5455.4/Rider EAP.app/Contents/MacOS/rider" $wait --args "${ideargs[@]}"