declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/David.Walters@AlaskaAir.com/Library/Application Support/JetBrains/Toolbox/apps/Rider/ch-0/223.7401.6/Rider EAP.app/Contents/MacOS/rider" $wait --args "${ideargs[@]}"