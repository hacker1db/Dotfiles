declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/David.Walters@AlaskaAir.com/Library/Application Support/JetBrains/Toolbox/apps/Rider/ch-0/222.3345.139/Rider.app/Contents/MacOS/rider" $wait --args "${ideargs[@]}"